JELLYFIN_API_KEY=
JELLYFIN_BASE_URL=http://jellyfin:8096
WLED_HOST=wled-ambilight-lgc1.lan
WLED_UDP_RAW_PORT=19446
PLAYBACK_MONITOR_INTERVAL=0.2
DNS_TTL_SECONDS=3600
DEVICE_MATCH_FIELD=DeviceName
# WLED_DEVICE_* mappings (examples):
# WLED_DEVICE_LGSMARTTV=wled-ambilight-lgc1.lan:19446
# WLED_DEVICE_ANDROIDTV=wled-tv.lan:19446

# Environment variables used by extractor daemon (ambilight-daemon-extractor.py)
LIBRARY_SCAN_INTERVAL=3600
EXTRACTION_PRIORITY=newest_first
EXTRACTION_BATCH_SIZE=5
EXTRACTION_START_TIME=00:00
EXTRACTION_END_TIME=23:59

AMBILIGHT_DATA_DIR=./data

# Environment variables used by player (ambilight-player/src/main.rs
AMBILIGHT_RGBW=false
AMBILIGHT_SYNC_LEAD_SECONDS=0.2
AMBILIGHT_SMOOTH_SECONDS=0.12
AMBILIGHT_GAMMA=2.2
AMBILIGHT_SATURATION=1.0
AMBILIGHT_BRIGHTNESS_TARGET=60.0
AMBILIGHT_ORDER=RGB
AMBILIGHT_GAMMA_RED=1.0
AMBILIGHT_GAMMA_GREEN=1.0
AMBILIGHT_GAMMA_BLUE=1.0
AMBILIGHT_RED_BOOST=3.0
AMBILIGHT_BLUE_BOOST=4.0
AMBILIGHT_GREEN_BOOST=1.0
# any led below this brightness will be black/off
AMBILIGHT_MIN_LED_BRIGHTNESS=0.0
# number of leds from the top left corner of
# the screen (looking from the front)
# where the first led is positioned
AMBILIGHT_INPUT_POSITION=0

# Target LED counts for resampling in the player
AMBILIGHT_TOP_LED_COUNT=89
AMBILIGHT_BOTTOM_LED_COUNT=89
AMBILIGHT_LEFT_LED_COUNT=49
AMBILIGHT_RIGHT_LED_COUNT=49
